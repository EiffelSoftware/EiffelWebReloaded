<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-6-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-6-0 http://www.eiffel.com/developers/xml/configuration-1-6-0.xsd" name="eiffelweb" uuid="6A03BC65-BD03-4B68-B51E-2543F9471D42">
	<target name="eiffelweb">
		<root cluster="application" class="APPLICATION_ROOT" feature="make"/>
		<setting name="console_application" value="true"/>
		<setting name="multithreaded" value="true"/>
		<file_rule>
			<exclude>/\.svn$</exclude>
			<exclude>/\.git$</exclude>
			<exclude>/EIFGENs$</exclude>
		</file_rule>
		
		<library name="base" location="$ISE_LIBRARY\library\base\base-safe.ecf"/>
		<library name="fcgi" location="..\fcgi.ecf"/>
		<cluster name="application" location="." recursive="true">
		</cluster>
	</target>
</system>
